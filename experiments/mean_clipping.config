# Run the same agent multiple times.
# We are interested in the shape of the average rewards curve.

starting_port = 4040
[instance_1]
    agent = PredictiveFutureAgent

    [[agent_args]]
        action_stdev = 5
        learning_rate = 10e-1
        clipping_range = 1000
        nn_hidden_size = 20

    [[exp_args]]
        dir = experiments/clipping
        agent_prefix = pfa1_20_5_10e-1
        num_epochs = 100
        epoch_length = 50000
        test_length = 50000

