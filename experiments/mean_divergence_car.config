# Run the same agent multiple times.
# We are interested in the shape of the average rewards curve.

starting_port = 4040
env_base_path = /home/rihards/Programming/Multi/rl/rl-library/rl-library/products
[instance_1]
    agent = PredictiveFutureAgent
    env_file = MountainCar.jar

    [[agent_args]]
        action_stdev = 5
        learning_rate = 10e-6
        nn_hidden_size = 20

    [[exp_args]]
        dir = experiments/mean_divergence_car
        agent_prefix = pfa1_20_5_10e-6
        num_epochs = 100
        epoch_length = 50000
        test_length = 50000

